<UserControl x:Class="assign3.Views.InitialView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:assign3.Views"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <Button Content="Master"
                HorizontalAlignment="Center" 
                Margin="0,155,0,0" 
                VerticalAlignment="Top" 
                Width="120" Height="40" 
                Command="{Binding NavigateMainCommand}"/>
        <Button Content="Bachelor" 
                HorizontalAlignment="Center"
                Margin="0,239,0,0" 
                VerticalAlignment="Top" 
                Width="120" Height="40"
                Command="{Binding NavigateBachelorCommand}"/>

    </Grid>
</UserControl>
